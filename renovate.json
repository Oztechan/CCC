{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "labels": [
    "dependencies"
  ],
  "rebaseWhen": "conflicted",
  "gitAuthor": "Renovate <renovate@oztechan.com>",
  "commitBody": "Co-authored-by: Mustafa Ozhan <mr.mustafa.ozhan@gmail.com>",
  "commitMessageAction": "[Oztechan/CCC#1457] Update",
  "prConcurrentLimit": 20,
  "git-submodules": {
    "enabled": true,
    "groupName": "Git Submodules"
  },
  "lockFileMaintenance": {
    "enabled": true,
    "commitMessageAction": "[Oztechan/CCC#1457] Lock file maintenance"
  },
  "packageRules": [
    {
      "groupName": "kotlin",
      "matchPackageNames": [
        "/^org.jetbrains.kotlin.$/"
      ]
    },
    {
      "groupName": "koin",
      "matchPackageNames": [
        "/^io.insert-koin/"
      ]
    }
  ]
}
